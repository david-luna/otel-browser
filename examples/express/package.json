    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@elastic/mockotlpserver": "^0.8.0",
    "@types/express": "^5.0.3"
  }
}
